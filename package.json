{
  "name": "nico-dev",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p clean fonts imagemin svgsprite webpack:dev fractal:start",
    "build": "npm-run-all clean fonts imagemin svgsprite --parallel webpack:build --sequential fractal:build",
    "webpack:dev": "webpack --mode=development --watch",
    "webpack:build": "webpack --mode production --progress",
    "fractal:start": "fractal start --sync",
    "fractal:build": "fractal build",
    "fonts": "cpx 'components/fonts/*.{eot,woff,woff2,ttf,svg,otf}' public/web/assets/fonts",
    "imagemin": "imagemin components/images/*.{jpg,png,gif,svg,ico} --out-dir=public/web/assets/images",
    "svgsprite": "svg-sprite --svg-xmldecl false --svg-namespace-ids true --symbol --symbol-sprite 'icon-sprite.svg' --symbol-example true components/images/./**/*.svg --symbol-dest public/web/assets/images/ --symbol-example-dest ../../components/01-pebble/icons/all-icons.twig --symbol-example-template components/01-pebble/icons/_sprite-preview-template.html",
    "clean": "del-cli public/web/assets/* build/* dist/*",
    "lint": "stylelint 'components/**/*.scss' --custom-syntax postcss-scss",
    "test": "run-s css:build lint",
    "deploy": "npm-run-all build deploy:github",
    "deploy:github": "gh-pages -d build"
  },
  "keywords": [],
  "author": "Nico Fortino",
  "license": "MIT",
  "dependencies": {
    "@frctl/fractal": "^1.5.15",
    "caniuse-lite": "^1.0.30001524",
    "classlist-polyfill": "^1.2.0",
    "element-closest": "^3.0.2",
    "eslint-config-airbnb": "^19.0.4",
    "gh-pages": "^5.0.0",
    "gsap": "^3.12.5",
    "imagemin": "^8.0.1",
    "lazysizes": "^5.3.2",
    "locomotive-scroll": "^4.1.4",
    "mdn-polyfills": "^5.20.0",
    "nodelist-foreach-polyfill": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "picturefill": "^3.0.3",
    "swiper": "^9.1.1"
  },
  "devDependencies": {
    "@frctl/twig": "^1.2.13",
    "array-from": "^2.1.1",
    "autoprefixer": "^10.4.14",
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.2",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-proto-to-assign": "^6.26.0",
    "bootstrap": "^5.2.2",
    "browserslist": "^4.21.4",
    "cpx": "^1.5.0",
    "css-loader": "^6.7.1",
    "del-cli": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-webpack-plugin": "^4.0.1",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "imagemin-cli": "^7.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.1.0",
    "postcss-scss": "^4.0.5",
    "sass": "^1.66.1",
    "sass-loader": "^13.0.2",
