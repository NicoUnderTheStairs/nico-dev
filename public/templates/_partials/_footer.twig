{% set footer = footer.footer %}
{% set header = header.header %}

<footer class="footer bg-primary js-footer">
    <div class="footer__inner p-3 p-lg-5">
        <h3 class="ufo__suckable mt-0 mono">{{ footer.slogan }}<span class="footer__easteregg c-p linkSmallCursor d-none">.</span></h3>

        <div class="footer__listwrapper mb-5">
            <ul class="footer__nav__list p-0">
                {% for item in header.nav %}
                    <li class="footer__nav__item mb-1">
                        <a href="{{ item.url }}" class="ufo__suckable footer__nav__link mono">{{ item.title }}</a>
                    </li>
                {% endfor %}
            </ul>   
        </div>

        <div class="footer__listwrapper">
            <ul class="footer__socials__list p-0">
                {% for item in footer.socials %}
                    <li class="footer__socials__item mb-1">
                        <a href="{{ item.sociallink }}" target="_blank" class="footer__socials__link mono linkSmallCursor ufo__suckable">
                            <svg width="18" height="16" viewBox="0 0 22 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <g clip-path="url(#clip0_697_135)">
                                <path d="M-1.88488 24.0686L-14.636 36.8198L-16.0503 35.4056L-3.29909 22.6544L-11.2187 22.6544L-11.201 20.6568H0.112698V31.9705L-1.88488 31.9882L-1.88488 24.0686Z" fill="#202020"/>
                                <path d="M15.1151 7.06861L2.36396 19.8198L0.949747 18.4056L13.7009 5.6544L5.78131 5.6544L5.79899 3.65682H17.1127V14.9705L15.1151 14.9882L15.1151 7.06861Z" fill="#202020"/>
                                </g>
                                <defs>
                                <clipPath id="clip0_697_135">
                                <rect width="22" height="20" fill="white"/>
                                </clipPath>
                                </defs>
                            </svg>
                            {{ item.titel }}
                        </a>
                    </li>
                {% endfor %}
            </ul>   
        </div>
    </div>
    <div class="footer__legal d-flex align-items-center justify-content-between px-3 pb-2 px-lg-5 pb-lg-2">
        <div class="d-flex justify-content-between">
            <p class="mono text-small ufo__suckable">Â© Nico</p>
            {# <span class="mono text-small mx-1"> </span> #}
            <div class="footer__lang d-flex align-items-center linkSmallCursor">
                {% for siteForEntry in entry.getSupportedSites %}
                    {% set site = craft.app.getSites.getSiteById(siteForEntry.siteId) %}
                    {% set entryForOtherSite = craft.entries.id(entry.id).site(site).one() %}
                    {% if entryForOtherSite %}
                        <a class="text-small footer__lang__active {{ site.id != entry.siteId ? ' footer__lang__inactive' }} ufo__suckable" href="{{ entryForOtherSite.getUrl() }}">
                            {{site.handle|upper}}
                        </a>

                        {% if loop.index0 == 0 %}<span class="mono text-small mx-1"> / </span>{% endif %}
                    {% endif %}
                {% endfor %}
            </div>
        </div>

            {# <a href="{{footer.imprint.one().url}}" class="mono text-small">{{footer.imprint.one().title}}</a> #}
